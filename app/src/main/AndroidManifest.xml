<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.americanorbritish">

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_americanorbritish"
        android:label="@string/app_name"
        android:roundIcon="@drawable/ic_americanorbritish"
        android:supportsRtl="true"
        android:theme="@style/AppTheme.NoActionBar">
        <activity android:name=".LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity" />
        <activity android:name=".ResetPasswordActivity" />
        <activity android:name=".SignUp" />
        <activity android:name=".QuizFoodActivity" />
        <activity android:name=".QuizTransportationActivity" />
        <activity android:name=".QuizClothesActivity" />
        <activity android:name=".QuizRandomActivity" />
        <activity android:name=".ResultActivity" />
        <activity android:name=".ScoreActivity" />
    </application>

</manifest>